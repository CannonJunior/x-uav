# X-UAV Environment Configuration
# Copy this file to .env and update values as needed

# Application
ENVIRONMENT=development
PORT=7676

# Database
DATABASE_URL=postgresql://xuav:development@localhost:5432/xuav

# ArangoDB Graph Database
GRAPH_DB_URL=http://localhost:8529
GRAPH_DB_NAME=xuav
GRAPH_DB_USERNAME=root
GRAPH_DB_PASSWORD=development

# Redis
REDIS_URL=redis://localhost:6379

# Ollama LLM
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=qwen2.5:3b

# MCP Server
MCP_SERVER_PATH=/app/mcp_server.py

# Security
SECRET_KEY=your-secret-key-change-in-production
ALLOWED_ORIGINS=http://localhost:7676,http://localhost:8000

# API Configuration
API_V1_PREFIX=/api/v1
